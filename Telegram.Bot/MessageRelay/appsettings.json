{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "TelegramBotClientSetting": {
    "Token": "{TELEGRAM_BOT_TOKEN}}",
    "ChatId": "{TELEGRAM_BOT_CHAT_ID}}"
